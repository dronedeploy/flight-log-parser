{
  "name": "@dronedeploy/flight-log-parser",
  "version": "2.2.0",
  "description": "Parser for DroneDeploy flight logs",
  "main": "dist/flight-log-parser.js",
  "module": "dist/flight-log-parser.es6.js",
  "typings": "dist/types/flight-log-parser.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://www.github.com/dronedeploy/flight-log-parser",
  "license": "MIT",
  "scripts": {
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest"
  },
  "jest": {
    "rootDir": "./",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "\\.(test|spec)\\.ts$",
    "moduleDirectories": [
      "node_modules",
      "src"
  ],
    "modulePaths": ["<rootDir>"],
    "moduleFileExtensions": [
      "ts",
      "js",
      "jsx"
    ],
    "moduleDirectories": ["node_modules", "bower_components", "src"]
  },
  "dependencies": {
    "csv-parse": "^4.4.6",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.2.2",
    "@types/jest": "^29.2.5",
    "@types/node": "^14.18.36",
    "jest": "^29.3.1",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.13.7",
    "rollup": "^0.58.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.11.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^6.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^4.9.4"
  }
}
